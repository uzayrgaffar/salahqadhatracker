{
  "build": {
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "autoIncrement": "version"
      },
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=8192"
      },
      "cache": {
        "disabled": true
      }
    }
  }
}